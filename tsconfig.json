{
  "compilerOptions": {
    "jsx": "react",
    "lib": ["dom", "es7"],
    "rootDir": "src",
    "module": "commonjs",
    "target": "es2017",
    "sourceMap": true,
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
		"sourceMap": true
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules",
    "build"
  ]
}
